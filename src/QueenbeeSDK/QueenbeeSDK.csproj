<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>NET452</TargetFramework>
    <AssemblyName>QueenbeeSDK</AssemblyName>
    <PackageId>QueenbeeSDK</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Ladybug Tools</Authors>
    <Company>Ladybug Tools</Company>
    <AssemblyTitle>QueenbeeSDK</AssemblyTitle>
    <Description>A library generated from a OpenAPI doc</Description>
    <Copyright>No Copyright</Copyright>
    <RootNamespace>QueenbeeSDK</RootNamespace>
    <Version>1.17.3</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\QueenbeeSDK.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/GIT_USER_ID/GIT_REPO_ID.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Minor update</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JsonSubTypes" Version="1.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="RestSharp" Version="106.11.4" />
    <PackageReference Include="Polly" Version="7.2.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

</Project>
